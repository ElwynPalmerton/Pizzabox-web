analyze:
  env:
    SONAR_TOKEN: ${{secrets.SONAR_TOKEN}}
  runs-on: macOS-10.14
  steps:
    - uses: actions/checkout@v2
    - run: dotnet tool install -g dotnet-sonarscanner
    - run: dotnet sonarscanner begin -k:'elwynpalmerton' -o:'ElwynPalmerton' -d.sonar.login=$SONAR_TOKEN -d.sonar.url:'https://sonarcloud.io'
    - run: dotnet build
    - run: dotnet sonarscanner end -d.sonar.login=$SONAR_TOKEN